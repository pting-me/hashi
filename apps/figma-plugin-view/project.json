{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/figma-plugin-view/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build --minify --emptyOutDir=false --config=apps/figma-plugin-view/vite.config.ts",
        "__unparsed__": []
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "figma-plugin-view:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "figma-plugin-view:build:development"
        },
        "production": {
          "buildTarget": "figma-plugin-view:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/figma-plugin-view/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/figma-plugin-view"],
      "options": {
        "jestConfig": "apps/figma-plugin-view/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
