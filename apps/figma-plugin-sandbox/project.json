{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/figma-plugin-sandbox/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/figma-plugin-sandbox",
        "command": "rollup --config rollup.config.ts --configPlugin rollup-plugin-esbuild"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/figma-plugin-sandbox",
        "command": "echo 'Nothing to serve'"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/figma-plugin-sandbox/**/*.ts"]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/figma-plugin-sandbox",
        "command": "vitest --run"
      }
    }
  },
  "tags": []
}
